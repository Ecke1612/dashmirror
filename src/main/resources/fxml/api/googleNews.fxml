<VBox xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="apis.google_news.GoogleNewsController">
   <children>
      <BorderPane>
         <left>
            <Button fx:id="btn_settings" maxHeight="24.0" maxWidth="24.0" mnemonicParsing="false" onAction="#settings" prefHeight="24.0" prefWidth="24.0" style="-fx-background-color: transparent;" BorderPane.alignment="CENTER">
               <graphic>
                  <ImageView fitHeight="24.0" fitWidth="24.0">
                     <image>
                        <Image url="@../../img/gui/ham_button.png" />
                     </image>
                  </ImageView>
               </graphic>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
            </Button>
         </left>
         <right>
            <Button fx:id="btn_delete" mnemonicParsing="false" onAction="#delete" text="X" BorderPane.alignment="CENTER" />
         </right>
         <center>
            <Label text="Google News" BorderPane.alignment="CENTER" />
         </center>
         <bottom>
            <Separator prefWidth="200.0" BorderPane.alignment="CENTER">
               <padding>
                  <Insets bottom="5.0" top="3.0" />
               </padding>
            </Separator>
         </bottom>
      </BorderPane>
      <VBox prefWidth="300.0">
         <children>
            <TitledPane expanded="false" graphicTextGap="0.0" style="-fx-background-color: transparent;">
              <content>
                <AnchorPane>
                     <children>
                        <Button fx:id="btn_toarticle_1" mnemonicParsing="false" onAction="#toarticle_1" style="-fx-background-color: transparent;">
                           <graphic>
                              <TextArea editable="false" prefHeight="101.0" prefWidth="300.0" style="-fx-background-color: #000000; -fx-border-color: #000000;" text="Tropical Storm Florence, which made landfall as a hurricane on Friday, was being blamed for at least 11 deaths as of Saturday night as its torrential rains continued to soak North Carolina and South Carolina during a slow march westward." wrapText="true" />
                           </graphic>
                        </Button>
                     </children>
                  </AnchorPane>
              </content>
               <graphic>
                  <TextArea editable="false" prefHeight="59.0" prefWidth="300.0" style="-fx-background-color: #000000; -fx-border-color: #000000;" text="Google News: Florence: At least 14 deaths reported as storm slogs across Carolinas" wrapText="true">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </TextArea>
               </graphic>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </TitledPane>
         </children>
      </VBox>
      <Separator prefWidth="200.0">
         <padding>
            <Insets bottom="2.0" top="2.0" />
         </padding>
      </Separator>
      <VBox prefWidth="300.0">
         <children>
