<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="apis.googlecalender.GoogleCalendarController">
   <children>
      <Label fx:id="label_head" text="Google Kalendar" textFill="WHITE" />
      <Label fx:id="label_head_date" text="Do. 13.09.2018" textFill="WHITE">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Separator prefWidth="200.0" style="-fx-background-color: #EEEEEE;" />
      <ScrollPane prefViewportHeight="300.0" prefViewportWidth="200.0" style="-fx-background: #000000; -fx-border-color: #000000;">
         <content>
            <VBox fx:id="vbox_main" spacing="10.0">
               <children>
                  <HBox spacing="10.0" />
               </children>
            </VBox>
         </content>
      </ScrollPane>
   </children>
</VBox>
